{
  "name": "judgepad",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:rlaakkol/judgepad.git",
  "scripts": {
    "start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "test:lint": "eslint ./src --ext .js --ext .jsx --cache",
    "package": "NODE_ENV=production webpack --progress --colors",
    "deploy": "npm run package && surge -p $(git rev-parse --show-toplevel)"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.2.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-google": "^0.7.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "jquery": "^2.2.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^15.4.2",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "apollo-client": "^0.8.7",
    "babel-preset-stage-1": "^6.1.18",
    "graphql-tag": "^1.2.4",
    "lodash": "^3.10.1",
    "moment": "^2.17.1",
    "node-uuid": "^1.4.7",
    "prettier": "^1.1.0",
    "react": "^15.4.2",
    "react-apollo": "^0.11.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^4.0.0",
    "react-router": "^2.0.1",
    "react-router-bootstrap": "^0.23.1",
    "redux": "^3.0.4",
    "redux-storage": "^4.1.2",
    "redux-storage-engine-localstorage": "^1.1.4"
  }
}
